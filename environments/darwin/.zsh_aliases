# Modern Unix
alias ls='eza -lah'
alias lss='ls --total-size --ignore-glob node_modules'
alias bat='bat --paging=never'
alias date='gdate'
alias df='duf'
alias find='fd'
alias top='gtop'
alias htop='gtop'
alias ping='gping'
alias ps='procs'
alias dig='q'
alias xargs='gxargs'
alias cz='chezmoi'
alias tar='gtar'
# Git
alias gpu='git pull --rebase --autostash'
alias gpf='git push --force-with-lease'
alias gsm='git stash > /dev/null && (git switch -q master 2> /dev/null || git switch -q main)'
alias gsmp='gsm && gpu'
alias gk='/opt/homebrew/bin/gk'
# Shortcuts
alias zshrc='nano ~/.local/share/chezmoi/environments/darwin/.zshrc && cz apply && source ~/.zshrc'
alias aliases='nano ~/.local/share/chezmoi/environments/darwin/.zsh_aliases && cz apply && source ~/.zshrc'
alias bu='brew update --verbose && brew outdated'
alias bg='brew upgrade'
alias local-gitignore='git local-ignore'
alias brew='~/brew-sbom.sh'
alias flushdns='sudo dscacheutil -flushcache && sudo killall -HUP mDNSResponder'
