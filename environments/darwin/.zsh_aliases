# Modern Unix
alias ls='eza -lah'
alias bat='bat --paging=never'
alias date='gdate'
alias df='duf'
alias find='fd'
alias top='gtop'
alias htop='gtop'
alias ping='gping'
alias ps='procs'
alias dig='q'
alias xargs='gxargs'
alias cz='chezmoi'
# Git
alias gpu='git pull --rebase --autostash'
alias gpf='git push --force-with-lease'
alias gsm='git stash > /dev/null && (git switch -q master 2> /dev/null || git switch -q main)'
alias gsmp='gsm && gpu'
# Shortcuts
alias zshrc='nano ~/.local/share/chezmoi/environments/darwin/.zshrc && cz apply && source ~/.zshrc'
alias aliases='nano ~/.local/share/chezmoi/environments/darwin/.zsh_aliases && cz apply && source ~/.zshrc'
alias bu='brew update --verbose && brew outdated'
alias bg='brew upgrade'
alias local-gitignore='code ./.git/info/exclude'
alias obtain-ytl-mfa='/Users/linusw/Documents/Projects/Reaktor/YTL/cdk-infra/obtain-ytl-mfa.sh'
alias ecr-login='/Users/linusw/Documents/Scripts/ecr-login.sh'
alias naksu2-start='npm run dev'
alias naksu2-start-staging='NAKSU2_RELEASE_INFO_URL=https://static.abitti.fi/naksu2/digabi2-versions-staging.json npm run dev'
alias naksu2-log='tail -f ./naksu2.log'
alias cdk='./ytl-cdk.sh'
alias brew='~/brew-sbom.sh'
